[
    { "key": "alt+b", "command": "workbench.action.toggleSidebarVisibility", },
    { "key": "alt+e", "command": "workbench.action.quickOpen", },
    { "key": "alt+d", "command": "workbench.view.explorer", },
    { "key": "alt+g", "command": "workbench.view.scm", },
    { "key": "alt+n", "command": "workbench.action.newWindow", },
    { "key": "alt+o", "command": "projectManager.listProjects", },
    { "key": "alt+p", "command": "workbench.action.showCommands", },
    { "key": "alt+y", "command": "workbench.action.toggleCenteredLayout", },
    { "key": "alt+;", "command": "workbench.action.terminal.toggleTerminal", },
    { "key": "alt+t", "command": "breadcrumbs.focusAndSelect",},

    { "key": "alt+h", "command": "breadcrumbs.focusPrevious", "when": "breadcrumbsActive && breadcrumbsVisible" },
    { "key": "alt+l", "command": "breadcrumbs.focusNext", "when": "breadcrumbsActive && breadcrumbsVisible" },

    { "key": "alt+i", "command": "workbench.action.splitEditor", "when": "editorFocus", },
    { "key": "alt+h", "command": "workbench.action.focusPreviousGroup", "when": "editorFocus", },
    { "key": "alt+j", "command": "workbench.action.previousEditorInGroup", "when": "editorFocus", },
    { "key": "alt+k", "command": "workbench.action.nextEditorInGroup", "when": "editorFocus", },
    { "key": "alt+l", "command": "workbench.action.focusNextGroup", "when": "editorFocus", },
    { "key": "alt+f", "command": "workbench.action.toggleEditorWidths", "when": "editorFocus", },

    { "key": "alt+i", "command": "workbench.action.terminal.split", "when": "terminalFocus" },
    { "key": "alt+h", "command": "workbench.action.terminal.focusPreviousPane", "when": "terminalFocus", },
    { "key": "alt+j", "command": "workbench.action.terminal.focusNext", "when": "terminalFocus" },
    { "key": "alt+k", "command": "workbench.action.terminal.focusPrevious", "when": "terminalFocus" },
    { "key": "alt+l", "command": "workbench.action.terminal.focusNextPane", "when": "terminalFocus" },
    { "key": "alt+n", "command": "workbench.action.terminal.new", "when": "terminalFocus" },
    {
        "key": "alt+f",
        "command": "workbench.action.toggleMaximizedPanel",
        "when": "terminalFocus"
    },
    {
        "key": "alt+enter",
        "command": "editor.action.revealDefinition",
        "when": "editorHasDefinitionProvider && editorTextFocus && !isInEmbeddedEditor"
    },
    {
        "key": "f12",
        "command": "-editor.action.revealDefinition",
        "when": "editorHasDefinitionProvider && editorTextFocus && !isInEmbeddedEditor"
    },
    {
        "key": "shift+alt+enter",
        "command": "editor.action.goToReferences",
        "when": "editorHasReferenceProvider && editorTextFocus && !inReferenceSearchEditor && !isInEmbeddedEditor"
    },
    {
        "key": "shift+f12",
        "command": "-editor.action.goToReferences",
        "when": "editorHasReferenceProvider && editorTextFocus && !inReferenceSearchEditor && !isInEmbeddedEditor"
    },
    {
        "key": "alt+left",
        "command": "workbench.action.navigateBack"
    },
    {
        "key": "ctrl+alt+-",
        "command": "-workbench.action.navigateBack"
    },
]
